

<%= simple_form_for @project do |f| %>
	<%= f.input :title %>
	<br>
	<%= f.input :description, :as => :text, :label => false, :input_html => {id: 'redactor1', :class => "redactor"}%>
	<br>
	<%= f.input :misc_html, :as => :text, :label => false, :input_html => {id: 'redactor2', :class => "redactor"}%>
	<br>
	<%= f.button :submit %>
<% end %>




<br>
<hr>
<br>
<ul>
<% @projects.each do |project| %>
	<li><%= link_to project.title, project %></li>
<% end %>
</ul>